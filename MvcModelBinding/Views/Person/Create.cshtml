@model MvcModelBinding.Models.Person

<h2>Create Person</h2>

<form asp-action="Create" method="post">
    <label>First Name:</label>
    <input asp-for="FirstName" /><br />

    <label>Last Name:</label>
    <input asp-for="LastName" /><br />

    <label>Age:</label>
    <input asp-for="Age" /><br />

    <h3>Address</h3>
    <label>Street:</label>
    <input asp-for="Address.Street" /><br />

    <label>City:</label>
    <input asp-for="Address.City" /><br />

    <label>Zip Code:</label>
    <input asp-for="Address.ZipCode" /><br />

    <button type="submit">Submit</button>
</form>
